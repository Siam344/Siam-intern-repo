{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "linebreak-style": ["error", "windows"],
      "no-console": "off",
      "quotes": ["error", "single"],
      "indent": ["error", 2],
      "no-var": "error",
      "prefer-const": "error"
    }
  }
  